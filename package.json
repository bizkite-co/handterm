{
  "name": "handterm",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "npm run clean && vite",
    "build:package": "cd packages/types && rm -rf dist && npm run build && cd - && npm install",
    "build": "npm run build:package && vite build",
    "clean": "rm -rf dist && npm cache verify",
    "inspect": "npx tsx scripts/inspect.ts",
    "list-types": "tsx list-types.ts > project-types.lst",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:file": "eslint $1 --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:save": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 -f json > eslint-results.json",
    "lint:get-files": "jq -c '[.[] | select(.errorCount > 0 or .warningCount > 0) | { filePath: .filePath, errorCount: .errorCount, warningCount: .warningCount }]' eslint-files.json > eslint-files.json",
    "lint:get-files-csv": "jq -r 'sort_by(.errorCount) | reverse | .[] | select(.errorCount > 0) | [.errorCount, .filePath] | @csv' eslint-files.json > eslint-files.csv",
    "lint:save-files": "npm run lint:save; sleep 2 && npm run lint:get-files && npm run lint:get-files-csv",
    "lint:fix-file": "eslint --fix",
    "lint:types": "tsc --noEmit",
    "lint:style": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "npm run test:unit; npm run test:e2e",
    "test:unit": "mkdir -p vitest-output; rm -rf vitest-output/*; vitest --run",
    "test:unit-save": "mkdir -p vitest-output; rm -rf vitest-output/*; vitest --run --reporter=json --outputFile=./vitest-output/vitest.json",
    "test:save": "npm run test > temp/workig-tree-test-results.txt",
    "test:ui": "vitest --ui",
    "test:e2e": "mkdir -p test-results; PLAYWRIGHT_OUTPUT_DIR=test-results PLAYWRIGHT_REPORTERS='junit' PLAYWRIGHT_JUNIT_OUTPUT_NAME='test-results/playwright.xml' PLAYWRIGHT_TEST=1 npx playwright test",
    "test:e2e-save": "echo $(date +'%Y%m%d%H%M%S') > temp/e2e_tests_workig-tree.txt && PLAYWRIGHT_TEST=1 npx playwright test >> temp/e2e_tests_workig-tree.txt",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run lint && npm run lint:types && npm run lint:style && npm run test",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "preview": "vite preview",
    "restart-status": "concurrently -n 'TYPES,LINT,CHECKLIST' -c 'bgBlue.bold,bgMagenta.bold,bgGreen.bold' \"npm run type-check 2>&1 | tee .type-errors.log\" \"npm run lint\" \"node scripts/checklist-status.js\"",
    "update-type-imports": "node scripts/dist/update-type-imports.js"
  },
  "config": {
    "checklist_path": "docs/worklog/reconcile-mock-types-with-prod/migrate-from-monaco-editor-react/full-rewrite-checklist.md"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "^3.x.x",
    "@emotion/react": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@preact/signals-react": "^3.0.1",
    "@webcontainer/api": "^1.5.1-internal.9",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "axios": "^1.8.3",
    "buffer": "^6.0.3",
    "canvas": "^3.1.0",
    "canvas-confetti": "^1.9.3",
    "canvas-mock": "^0.0.0",
    "dotenv": "^16.4.7",
    "monaco-editor": "^0.52.2",
    "monaco-vim": "^0.4.2",
    "raw-body": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "react-xtermjs": "^1.0.9",
    "recast": "^0.23.11",
    "styled-components": "^6.1.15",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "devDependencies": {
    "@amiceli/vitest-cucumber": "^4.7.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@cucumber/cucumber": "^11.2.0",
    "@handterm/types": "file:packages/types",
    "@playwright/test": "^1.51.0",
    "@tanstack/react-query": "^5.68.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "latest",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "buffer": "^6.0.3",
    "canvas": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.0",
    "eslint-plugin-custom-rules": "^0.0.0",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-total-functions": "^7.1.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "latest",
    "prettier": "^3.5.3",
    "react-docgen": "^7.1.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typedoc": "^0.28.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "latest"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
