{
  "name": "handterm",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "npm run clean && vite",
    "build:package": "cd packages/types && rm -rf dist && npm run build && cd - && npm install",
    "build": "npm run build:package && vite build",
    "clean": "rm -rf dist && npm cache verify",
    "list-types": "tsx list-types.ts > project-types.lst",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:file": "eslint $1 --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:save": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 -f json > eslint-results.json",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:get-files": "jq -c '[.[] | select(.errorCount > 0 or .warningCount > 0) | { filePath: .filePath, errorCount: .errorCount, warningCount: .warningCount }]' eslint-results.json > eslint-files.json",
    "lint:get-files-csv": "jq -r 'sort_by(.errorCount) | reverse | .[] | select(.errorCount > 0) | [.errorCount, .filePath] | @csv' eslint-files.json > eslint-files.csv",
    "lint:save-files": "npm run lint:save; sleep 2 && npm run lint:get-files && npm run lint:get-files-csv",
    "lint:fix-file": "eslint --fix",
    "lint:types": "tsc --noEmit",
    "lint:style": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test:unit": "vitest --run",
    "test": "vitest --run && npm run test:e2e",
    "test:ui": "vitest --ui",
    "test:e2e": "PLAYWRIGHT_TEST=1 npx playwright test",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run lint && npm run lint:types && npm run lint:style && npm run test",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "preview": "vite preview",
    "restart-status": "concurrently -n 'TYPES,LINT,CHECKLIST' -c 'bgBlue.bold,bgMagenta.bold,bgGreen.bold' \"npm run type-check 2>&1 | tee .type-errors.log\" \"npm run lint\" \"node scripts/checklist-status.js\"",
    "update-type-imports": "node scripts/dist/update-type-imports.js"
  },
  "config": {
    "checklist_path": "docs/worklog/reconcile-mock-types-with-prod/migrate-from-monaco-editor-react/full-rewrite-checklist.md"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "^3.x.x",
    "@emotion/react": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@preact/signals-react": "^2.2.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "canvas": "^2.11.2",
    "canvas-confetti": "^1.9.3",
    "canvas-mock": "^0.0.0",
    "dotenv": "^16.4.5",
    "monaco-editor": "^0.52.0",
    "monaco-vim": "^0.4.2",
    "raw-body": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.27.0",
    "react-xtermjs": "^1.0.9",
    "recast": "^0.23.9",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "devDependencies": {
    "@amiceli/vitest-cucumber": "^4.2.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@cucumber/cucumber": "^11.1.0",
    "@handterm/types": "file:packages/types",
    "@playwright/test": "^1.49.1",
    "@tanstack/react-query": "^5.59.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "buffer": "^6.0.3",
    "canvas": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-custom-rules": "^0.0.0",
    "eslint-plugin-functional": "^5.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-total-functions": "^7.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "gh-pages": "^6.2.0",
    "glob": "^10.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.3.3",
    "react-docgen": "^7.0.3",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.25.13",
    "typescript": "~5.4.0",
    "vite": "^5.3.2",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
